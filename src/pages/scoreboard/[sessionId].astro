---
import { ScoreboardWithProvider } from '@/components/ScoreboardWithProvider';
import Layout from '../../layouts/Layout.astro';

const { sessionId } = Astro.params;

if (!sessionId) {
  return Astro.redirect('/');
}
---

<Layout title="Scoreboard - Perfil Game">
	<ScoreboardWithProvider client:only="react" sessionId={sessionId} />
</Layout>
