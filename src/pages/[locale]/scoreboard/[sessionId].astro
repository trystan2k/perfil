---
import { ScoreboardWithProvider } from '@/components/ScoreboardWithProvider';
import Layout from '../../../layouts/Layout.astro';

const { sessionId } = Astro.params;

if (!sessionId) {
  return Astro.redirect('/');
}
---

<Layout title="scoreboard.title">
	<ScoreboardWithProvider client:load sessionId={sessionId} />
</Layout>
