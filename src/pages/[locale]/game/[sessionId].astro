---
import { GamePlayWithProvider } from '../../../components/GamePlayWithProvider';
import Layout from '../../../layouts/Layout.astro';

const { sessionId } = Astro.params;

if (!sessionId) {
  return Astro.redirect('/');
}
---

<Layout title="Perfil - Game Session">
	<GamePlayWithProvider sessionId={sessionId} client:only="react" />
</Layout>
