{
  "name": "perfil",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "biome check --error-on-warnings .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit && astro check --minimumFailingSeverity hint",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --config=e2e/playwright.config.ts",
    "complete-check": "pnpm astro sync && pnpm run lint && pnpm run typecheck && pnpm run test:coverage && pnpm run test:e2e && pnpm run build",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,astro,json,css}": [
      "biome format --write",
      "biome check --write --error-on-warnings"
    ],
    "e2e/**/*.{ts}": [
      "biome format --write",
      "biome check --write --error-on-warnings"
    ],
    "astro.config.mjs": [
      "biome format --write",
      "biome check --write --error-on-warnings"
    ],
    "tailwind.config.mjs": [
      "biome format --write",
      "biome check --write --error-on-warnings"
    ],
    "biome.json": [
      "biome format --write",
      "biome check --write --error-on-warnings"
    ],
    "vitest.config.*": [
      "biome format --write",
      "biome check --write --error-on-warnings"
    ],
    "package.json": [
      "biome format --write",
      "biome check --write --error-on-warnings"
    ],
    "public/**/*.json": [
      "biome format --write",
      "biome check --write --error-on-warnings"
    ],
    "src/**/*.{ts,tsx}": [
      "vitest related --run"
    ]
  },
  "dependencies": {
    "@astrojs/cloudflare": "~12.6.10",
    "@astrojs/react": "~4.4.2",
    "@astrojs/tailwind": "~6.0.2",
    "@radix-ui/react-dialog": "~1.1.15",
    "@radix-ui/react-label": "~2.1.8",
    "@radix-ui/react-popover": "~1.1.15",
    "@radix-ui/react-progress": "~1.1.8",
    "@radix-ui/react-slot": "~1.2.4",
    "@tanstack/react-query": "~5.90.7",
    "astro": "~5.15.9",
    "class-variance-authority": "~0.7.1",
    "clsx": "~2.1.1",
    "framer-motion": "~12.23.24",
    "idb": "~8.0.3",
    "lucide-react": "~0.553.0",
    "react": "~19.2.0",
    "react-dom": "~19.2.0",
    "tailwind-merge": "~3.4.0",
    "tailwindcss": "~3.4.18",
    "tailwindcss-animate": "~1.0.7",
    "zod": "~4.1.12",
    "zustand": "~5.0.8"
  },
  "devDependencies": {
    "@astrojs/check": "~0.9.5",
    "@biomejs/biome": "~2.3.4",
    "@playwright/test": "~1.56.1",
    "@tailwindcss/container-queries": "~0.1.1",
    "@testing-library/jest-dom": "~6.9.1",
    "@testing-library/react": "~16.3.0",
    "@testing-library/user-event": "~14.6.1",
    "@types/react": "~19.2.2",
    "@types/react-dom": "~19.2.2",
    "@vitejs/plugin-react": "~5.1.0",
    "@vitest/coverage-istanbul": "~4.0.8",
    "happy-dom": "~20.0.10",
    "husky": "~9.1.7",
    "jsdom": "~27.1.0",
    "lint-staged": "~16.2.6",
    "playwright": "~1.56.1",
    "typescript": "~5.9.3",
    "vite-plugin-pwa": "~1.1.0",
    "vitest": "~4.0.8",
    "workbox-window": "~7.3.0",
    "wrangler": "~4.46.0"
  }
}
